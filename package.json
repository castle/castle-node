{
  "$schema": "http://json.schemastore.org/package",
  "name": "@castleio/sdk",
  "description": "Castle SDK for Node",
  "version": "1.1.1",
  "main": "dist/index.js",
  "repository": "git@github.com:castle/castle-node.git",
  "author": {
    "name": "castle.io",
    "url": "https://castle.io"
  },
  "homepage": "https://github.com/castle/castle-node",
  "license": "MIT",
  "private": false,
  "scripts": {
    "clean": "rimraf dist",
    "lint-verify": "tslint -c tslint.json 'src/**/*.ts'",
    "lint-fix": "tslint -c tslint.json --fix '{src,test}/**/*.ts'",
    "build": "tsc",
    "start": "tsc -w",
    "test": "jest --runInBand",
    "prettier-fix": "prettier --write --parser typescript '{src,test}/**/*.ts'",
    "prettier-verify": "yarn prettier --list-different --parser typescript src/**/*.ts",
    "prepush": "yarn build && yarn lint-fix && yarn prettier-verify",
    "prepublishOnly": "yarn build"
  },
  "files": [
    "dist/*"
  ],
  "husky": {
    "hooks": {
      "pre-push": "yarn prepush"
    }
  },
  "devDependencies": {
    "@types/fetch-mock": "7.3.5",
    "@types/jest": "27.4.1",
    "@types/lodash.get": "4.4.6",
    "@types/lodash.isempty": "4.4.6",
    "@types/lodash.merge": "4.6.6",
    "@types/lodash.pickby": "4.6.6",
    "@types/lodash.reduce": "4.6.6",
    "@types/mockdate": "3.0.0",
    "@types/node": "17.0.22",
    "@types/node-fetch": "2.6.1",
    "@types/pino": "7.0.5",
    "fetch-mock": "9.11.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "mockdate": "3.0.5",
    "prettier": "2.6.0",
    "rimraf": "3.0.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "27.1.3",
    "ts-node": "10.7.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "9.0.0",
    "typescript": "4.6.2"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "lodash.get": "^4.4.2",
    "lodash.isempty": "^4.4.0",
    "lodash.merge": "^4.6.2",
    "lodash.pickby": "^4.6.0",
    "lodash.reduce": "^4.6.0",
    "node-fetch": "^2.6.1",
    "pino": "^6.11.3",
    "pino-pretty": "^5.0.2"
  }
}
