{
  "$schema": "http://json.schemastore.org/package",
  "name": "@castleio/sdk",
  "version": "0.4.6",
  "main": "dist/index.js",
  "repository": "git@github.com:castle/castle-node.git",
  "author": "castle.io",
  "license": "MIT",
  "private": false,
  "scripts": {
    "clean": "rimraf dist",
    "lint": "tslint -c tslint.json --fix 'src/**/*.ts'",
    "build": "tsc",
    "start": "tsc -w",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "prettier": "prettier --write --parser typescript src/**/*.ts",
    "prettier-check": "yarn prettier --list-different --parser typescript src/**/*.ts",
    "prepush": "yarn build && yarn lint && yarn prettier-check",
    "prepublishOnly": "yarn build"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn prepush"
    }
  },
  "devDependencies": {
    "@types/chai": "4.2.11",
    "@types/chai-as-promised": "7.1.2",
    "@types/fetch-mock": "7.3.2",
    "@types/lodash": "4.14.156",
    "@types/mocha": "7.0.2",
    "@types/node": "14.0.13",
    "@types/node-fetch": "2.5.7",
    "@types/pino": "6.3.0",
    "@types/sinon": "9.0.4",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "fetch-mock": "9.10.2",
    "husky": "4.2.5",
    "mocha": "8.0.1",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "sinon": "9.0.2",
    "source-map-support": "0.5.19",
    "ts-node": "8.10.2",
    "tslint": "6.1.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "9.0.0",
    "typescript": "3.9.5"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "pino": "^5.13.6",
    "pino-pretty": "^3.4.0"
  }
}
